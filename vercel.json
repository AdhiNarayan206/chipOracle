{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "backend/app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}